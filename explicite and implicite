public class ImplicitCasting {
    public static void main(String[] args) {
        int a = 10;
        double b = a; 
        System.out.println(a);
        System.out.println(b);
    }
}
public class ExplicitCasting {
    public static void main(String[] args) {
        double x = 10.75;
        int y = (int) x; 
        System.out.println(x);
        System.out.println(y);
    }
}
