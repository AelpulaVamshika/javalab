class Parent {
    String name;
    int age;

    Parent(String n, int a) {
        name = n;
        age = a;
    }

    void display() {
        System.out.println(name + " " + age);
    }
}

class Child extends Parent {
    String course;

    Child(String n, int a, String c) {
        super(n, a);
        course = c;
    }

    void show() {
        display();
        System.out.println(course);
    }
}

public class SuperWithParameters {
    public static void main(String[] args) {
        Child c = new Child("Vamshika", 20, "CSE");
        c.show();
    }
}
